@model TARge21Shop.Models.RealEstate.RealEstateDetailsViewModel

<h1>Details</h1>

<div>
    <h4>RealEstates</h4>
    <hr />
    <dl class="row">
        <div class="form-group row">
            <label asp-for="Address" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Address)
        </div>

        <div class="form-group row">
            <label asp-for="City" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.City)
        </div>

        <div class="form-group row">
            <label asp-for="Region" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Region)
        </div>

        <div class="form-group row">
            <label asp-for="PostalCode" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.PostalCode)
        </div>

        <div class="form-group row">
            <label asp-for="Country" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Country)
        </div>

        <div class="form-group row">
            <label asp-for="Phone" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Phone)
        </div>

        <div class="form-group row">
            <label asp-for="Fax" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Fax)
        </div>
        <div class="form-group row">
            <label asp-for="Size" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Size)
        </div>
        <div class="form-group row">
            <label asp-for="Floor" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Floor)
        </div>
        <div class="form-group row">
            <label asp-for="Price" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Price)
        </div>
        <div class="form-group row">
            <label asp-for="RoomCount" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.RoomCount)
        </div>

    </dl>
    <a asp-action="Index" asp-controller="realEstates" class="btn btn-danger">Back</a>
</div>